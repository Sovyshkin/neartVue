{
  "kind": "collectionType",
  "collectionName": "feedbacks",
  "info": {
    "singularName": "feedback",
    "pluralName": "feedbacks",
    "displayName": "Feedback",
    "description": "User feedback messages"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email",
      "required": true
    },
    "message": {
      "type": "text",
      "required": true
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user"
    }
  }
}